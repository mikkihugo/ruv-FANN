# Server Configuration
PORT=3000
NODE_ENV=development

# Database
DATABASE_URL=./database.sqlite

# JWT Configuration
JWT_SECRET=your-secret-key-here-change-in-production
JWT_EXPIRES_IN=7d

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100

# Logging
LOG_LEVEL=info
LOG_DIR=./logs

# Metrics
ENABLE_METRICS=true
METRICS_PORT=9090

# ruv-swarm Integration
RUV_SWARM_ENABLED=true